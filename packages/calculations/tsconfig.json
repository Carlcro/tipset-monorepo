{
  "compilerOptions": {
    "target": "es2017",

    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "moduleResolution": "node",
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "noEmit": true,
    "strict": true
  },
  "exclude": [
    "node_modules",
    "src/matchGroup/Euro/*",
    "src/points/Euro/*",
    "src/results/Euro/*"
  ]
}
